{
  "format_version": "1.13.0",
  "minecraft:feature_rules": {
    "description": {
      "identifier": "badger:rule_ore_redstone_patch_small_pvp",
      "places_feature": "badger:feature_ore_redstone_patch_small_aggregate"
    },
    "conditions": {
      "enabled": true,
      "placement_pass": 3,
      "minecraft:biome_filter": [
        {
          "any_of": [
            {
              "test": "has_biome_tag",
              "operator": "==",
              "value": "resource_redstone"
            }
          ]
        }
      ]
    },
    "distribution": {
      "iterations": "1",
      "x": {
        "distribution": "uniform",
        "extent": [
          0,
          15
        ]
      },
      "y": "query.heightmap(variable.worldx, variable.worldz)",
      "z": {
        "distribution": "uniform",
        "extent": [
          0,
          15
        ]
      },
      "scatter_chance": "q.custom_redstone_density * q.get_game_rule_value('custom_redstone_density')"
    }
  }
}