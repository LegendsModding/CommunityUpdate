{
  "animation_controllers": {
    "controller.animation.piglin_wheel_nis.default": {
      "initial_state": "init",
      "states": {
        "death": {
          "animations":  [
            {
              "death_a": 1.0
            }
          ],
          "on_exit": ["variable.face_anim_frame = 0;"]
        },
        "idle": {
          "animations":  [
            {
              "idle_a": 1.0
            }
          ],
          "on_exit": ["variable.face_anim_frame = 0;"],
          "transitions":  [
            {
              "walk": "(query.speed > 0)"
            },
            {
              "death": "variable.is_dead"
            }
          ]
        },
        "init": {
          "on_exit": ["variable.face_anim_frame = 0;"],
          "transitions":  [
            {
              "spawn": "!variable.skip_spawn"
            },
            {
              "idle": "variable.skip_spawn"
            }
          ]
        },
        "spawn": {
          "animations":  [
            {
              "spawn_a": 1.0
            }
          ],
          "on_exit": ["variable.face_anim_frame = 0;"],
          "transitions":  [
            {
              "idle": "query.any_animation_finished"
            },
            {
              "death": "variable.is_dead"
            }
          ]
        },
        "walk": {
          "animations":  [
            {
              "run_a": 1.0
            }
          ],
          "blend_transition": 0.2,
          "blend_via_shortest_path": true,
          "on_exit": ["variable.face_anim_frame = 0;"],
          "transitions":  [
            {
              "idle": "(query.speed <= 0)"
            },
            {
              "death": "variable.is_dead"
            }
          ]
        }
      }
    }
  },
  "format_version": "1.10.0"
}
