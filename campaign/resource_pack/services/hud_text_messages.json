{
   /* Priority values (see TextPacket::DisplayPriority in C++)
        -3 = CHAT (ie. message log -- middle left)
        -2 = HIDDEN
        0 = low (top left)
        1 = medium (top left)
        2 = high (center screen)
        3 = compass (ie. biome names - used only in C++)
        4 = tutorial (bottom right)
        5 = error (center screen)
        6 = onboarding (middle right)
        7 = songbook (attached to songbook icon top left)
        8 = map (attached to map icon top left)
  */

  /*
        ==== REQUIRED ====
        "message",
        "priority",
        ==== OPTIONAL ====
        "display_time",
        "title",
        "icon_path", ******** NEED TO SPECIFY .png or .gif ***********
        "looping",
        "is_alert",
        "is_celebratory",
        "persist"
  */

  /*
        ==== Notification Sequences ====
        "name_of_sequence": [
          {
            "id": "name_of_hud_message",
            "type": 0 (notification) / 1 (animation) / 2 (subtitle),
            "duration": 10 (seconds)
          },
        ]
  */

